{
  "name": "margiet-whale",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/margiet-whale/src",
  "tags": [
    "type:project",
    "scope:shared"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/margiet-whale",
        "index": "apps/margiet-whale/src/index.html",
        "main": "apps/margiet-whale/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/margiet-whale/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/margiet-whale/public"
          }
        ],
        "styles": [
          "@angular/material/prebuilt-themes/cyan-orange.css",
          "apps/margiet-whale/src/styles.scss"
        ],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/margiet-whale/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/margiet-whale/src/environment/environment.ts",
              "replaceWith": "apps/margiet-whale/src/environment/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "progress": true,
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "development"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "margiet-whale:build:production"
        },
        "development": {
          "buildTarget": "margiet-whale:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "margiet-whale:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/margiet-whale/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "margiet-whale:build",
        "port": 4200,
        "spa": true
      }
    }
  }
}
